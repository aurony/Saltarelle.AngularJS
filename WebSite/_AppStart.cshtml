@{    
   System.Web.Routing.RouteTable.Routes.MapWebPageRoute("api/{methodname}",      "~/api.cshtml", constraints: new { methodname = @"^[^\d\W]\w*\Z" } );
   System.Web.Routing.RouteTable.Routes.MapWebPageRoute("api/{methodname}/{id}", "~/api.cshtml", constraints: new { methodname = @"^[^\d\W]\w*\Z", id = @"\w*\Z" });
}