@{      
   var resource = Context.GetRouteValue("resource");          
   var id = Context.GetRouteValue("id");  
      
   if(resource=="person")
   {
      if(Request.HttpMethod=="GET" && id!=null)
      {
         var ob = new { id=10, name="Ham!" };
         Response.Write(Json.Encode(ob));        
      }
   }
   else
   {
      Response.SetStatus(404);
   }
}
